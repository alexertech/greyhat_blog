# Ignore bundler config.
/.bundle

# Ignore RubyMine
/.idea

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/database.yml
/config/secrets.yml
/config/credentials.yml.enc
/config/initializers/recaptcha.rb
sync.sh
server.sh

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

